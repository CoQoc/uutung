<mapper namespace="com.sist.erp.dao.ProductDAO">
	<select id="getProducts" resultType="com.sist.erp.vo.ProductVO" >
		SELECT * FROM PRODUCT WHERE NAME NOT LIKE 'TEST' ORDER BY TO_NUMBER(PSEQ)
	</select>
</mapper>